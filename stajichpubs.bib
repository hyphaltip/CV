	Number = {7},
	Owner = {stajich},
	Pages = {878--883},
	Pii = {EC.00083-14},
	Pmid = {24813190},
	Timestamp = {2014.07.05},
	Title = {Literature-based gene curation and proposed genetic nomenclature for \textit{Cryptococcus}.},

	Keywords = {Evolution, Molecular; Genomics, methods; Likelihood Functions; Models, Genetic; Multigene Family, genetics; Phylogeny; Saccharomyces, genetics; Stochastic Processes},
	Month = {Aug},
	Number = {8},

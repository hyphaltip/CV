	Institution = {Department of Biological Sciences, University of Idaho, Moscow, Idaho, United States of America.},
	Journal = {{PLoS Pathog}},
	Keywords = {Amphibians, microbiology; Animals; Aspartic Acid Proteases, genetics; Base Sequence; Biological Evolution; Chytridiomycota, genetics/growth /&/ development/pathogenicity; Evolution, Molecular; Genome, Fungal; Metalloproteases, genetics; Molecular Sequence Data; Mycoses, microbiology; Sequence Analysis, DNA; Serine Proteases, genetics},
	Language = {eng},
	Medline-Pst = {ppublish},

	Journal = {Genetics},
	Keywords = {Chromosomes, Fungal; Codon; Fertility, genetics; Gene Rearrangement; Genes, Mating Type, Fungal; Genome, Fungal; Neurospora, genetics/metabolism; Recombination, Genetic},
	Language = {eng},

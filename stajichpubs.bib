	Keywords = {Animals; Drosophila, genetics; Genetic Speciation; Genetics, Population, statistics /&/ numeric/al data; Likelihood Functions; Markov Chains; Models, Genetic; Monte Carlo Method; Mutation},
	Month = {Nov},
	Number = {3},

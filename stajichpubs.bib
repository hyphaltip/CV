	Pmid = {25351875},
	Timestamp = {2014.12.11},
	Title = {Comparative genomics of the major fungal agents of human and animal {Sporotrichosis}: \textit{Sporothrix schenckii} and \textit{Sporothrix brasiliensis}.},

	Pages = {1548--1553},
	Pii = {S0960-9822(13)00782-3},
	Pmid = {23932404},
	Timestamp = {2013.08.23},
	Title = {Shared Signatures of Parasitism and Phylogenomics Unite {Cryptomycota} and {Microsporidia}.},
	Url = {http://dx.doi.org/10.1016/j.cub.2013.06.057},
	Volume = {23},

@Article{Ouyang2014,
  author          = {Ouyang, Shouqiang and Park, Gyungsoon and Atamian, Hagop S. and Han, Cliff S. and \textbf{Stajich}, Jason E. and Kaloshian, Isgouhi and Borkovich, Katherine A.},
  journal         = {{PLoS} pathogens},

	Journal = {Genetics},
	Keywords = {Animals; Drosophila, genetics; Genetic Speciation; Genetics, Population, statistics /&/ numeric/al data; Likelihood Functions; Markov Chains; Models, Genetic; Monte Carlo Method; Mutation},
	Month = {Nov},
	Number = {3},
	Owner = {stajich},
	Pages = {1419--1436},
	Pii = {genetics.104.040402},
	Pmid = {16143628},
	Timestamp = {2009.06.11},
	Title = {Likelihoods from summary statistics: recent divergence between species.},
	Url = {http://dx.doi.org/10.1534/genetics.104.040402},

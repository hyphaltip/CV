	Pmid = {27294409},
	Timestamp = {2016.08.15},
	Title = {Transposable Elements versus the Fungal Genome: Impact on Whole-Genome Architecture and Transcriptional Profiles.},
	Url = {http://dx.doi.org/10.1371/journal.pgen.1006108},
	Volume = {12},

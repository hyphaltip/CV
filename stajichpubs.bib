	Publisher = {Cold Spring Harbor Labs Journals},
	Timestamp = {2016.09.25},
	Title = {{FGMP}: assessing fungal genome completeness and gene content},
	Url = {http://biorxiv.org/content/early/2016/04/20/049619},
	Year = {2016},
	Bdsk-Url-1 = {http://biorxiv.org/content/early/2016/04/20/049619},

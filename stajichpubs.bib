	Pmid = {23530226},
	Timestamp = {2013.05.08},
	Title = {Regional control of histone {H3} lysine 27 methylation in \textit{Neurospora}.},
	Url = {http://dx.doi.org/10.1073/pnas.1303750110},
	Volume = {110},

	Number = {6},
	Owner = {stajich},
	Pages = {901--906},
	Pii = {msg096},
	Pmid = {12716998},
	Timestamp = {2009.06.11},
	Title = {The effects of selection against spurious transcription factor binding sites.},
	Url = {http://dx.doi.org/10.1093/molbev/msg096},
	Volume = {20},
	Year = {2003},

  keywords        = {Alleles; Binding Sites; Fusarium, metabolism; Lycopersicon esculentum, virology; MicroRNAs, metabolism; Nucleotides, metabolism; Signal Transduction, genetics; Tobamovirus},
  nlm-id          = {101238921},

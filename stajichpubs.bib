	Institution = {Department of Biology, McGill University, Stewart Biology Building, 1205 Docteur Penfield Ave, Montreal, QC, Canada H3A 1B1.},
	Journal = {J Mol Biol},
	Keywords = {Amino Acid Sequence; Conserved Sequence; Evolution, Molecular; Genes, Fungal; Molecular Sequence Data; Phylogeny; Prions, chemistry/genetics; Protein Structure, Tertiary; Proteome, chemistry/genetics; Saccharomyces cerevisiae Proteins, chemistry/genetics; Saccharomycetales, genetics; Sequence Homology, Amino Acid},

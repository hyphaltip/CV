@Article{Kaemper2006,
  author      = {J\"{o}rg K\"{a}mper and Regine Kahmann and Michael B\"{o}lker and Li-Jun Ma and Thomas Brefort and Barry J Saville and Flora Banuett and James W Kronstad and Scott E Gold and Olaf M\"{u}ller and Michael H Perlin and Han A B W\"{o}sten and Ronald de Vries and Jos\'{e} Ruiz-Herrera and Cristina G Reynaga-Pe\~{n}a and Karen Snetselaar and Michael McCann and Jos\'{e} P\'{e}rez-Mart\'{i}n and Michael Feldbr\"{u}gge and Christoph W Basse and Gero Steinberg and Jose I Ibeas and William Holloman and Plinio Guzman and Mark Farman and Jason E \textbf{Stajich} and Rafael Sentandreu and Juan M Gonz\'{a}lez-Prieto and John C Kennell and Lazaro Molina and Jan Schirawski and Artemio Mendoza-Mendoza and Doris Greilinger and Karin M\"{u}nch and Nicole R\"{o}ssel and Mario Scherer and Miroslav Vranes and Oliver Ladendorf and Volker Vincon and Uta Fuchs and Bj\"{o}rn Sandrock and Shaowu Meng and Eric C H Ho and Matt J Cahill and Kylie J Boyce and Jana Klose and Steven J Klosterman and Heine J Deelstra and Lucila Ortiz-Castellanos and Weixi Li and Patricia Sanchez-Alonso and Peter H Schreier and Isolde H\"{a}user-Hahn and Martin Vaupel and Edda Koopmann and Gabi Friedrich and Hartmut Voss and Thomas Schl\"{u}ter and Jonathan Margolis and Darren Platt and Candace Swimmer and Andreas Gnirke and Feng Chen and Valentina Vysotskaia and Gertrud Mannhaupt and Ulrich G\"{u}ldener and Martin M\"{u}nsterk\"{o}tter and Dirk Haase and Matthias Oesterheld and Hans-Werner Mewes and Evan W Mauceli and David DeCaprio and Claire M Wade and Jonathan Butler and Sarah Young and David B Jaffe and Sarah Calvo and Chad Nusbaum and James Galagan and Bruce W Birren},
  journal     = {Nature},
  title       = {Insights from the genome of the biotrophic fungal plant pathogen \textit{Ustilago maydis}.},
  year        = {2006},
  month       = {Nov},
  number      = {7115},
  pages       = {97--101},
  volume      = {444},
  abstract    = {Ustilago maydis is a ubiquitous pathogen of maize and a well-established model organism for the study of plant-microbe interactions. This basidiomycete fungus does not use aggressive virulence strategies to kill its host. U. maydis belongs to the group of biotrophic parasites (the smuts) that depend on living tissue for proliferation and development. Here we report the genome sequence for a member of this economically important group of biotrophic fungi. The 20.5-million-base U. maydis genome assembly contains 6,902 predicted protein-encoding genes and lacks pathogenicity signatures found in the genomes of aggressive pathogenic fungi, for example a battery of cell-wall-degrading enzymes. However, we detected unexpected genomic features responsible for the pathogenicity of this organism. Specifically, we found 12 clusters of genes encoding small secreted proteins with unknown function. A significant fraction of these genes exists in small gene families. Expression analysis showed that most of the genes contained in these clusters are regulated together and induced in infected tissue. Deletion of individual clusters altered the virulence of U. maydis in five cases, ranging from a complete lack of symptoms to hypervirulence. Despite years of research into the mechanism of pathogenicity in U. maydis, no 'true' virulence factors had been previously identified. Thus, the discovery of the secreted protein gene clusters and the functional demonstration of their decisive role in the infection process illuminate previously unknown mechanisms of pathogenicity operating in biotrophic fungi. Genomic analysis is, similarly, likely to open up new avenues for the discovery of virulence determinants in other pathogens.},
  bdsk-url-1  = {http://dx.doi.org/10.1038/nature05248},

	Number = {5},
	Owner = {stajich},
	Pages = {997--1004},
	Pii = {rna.047985.114},
	Pmid = {25805857},
	Timestamp = {2015.05.06},
	Title = {The low information content of \textit{Neurospora} splicing signals: implications for {RNA} splicing and intron origin.},
	Url = {http://dx.doi.org/10.1261/rna.047985.114},
	Volume = {21},
